#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kamen.kolev/Documents/Projects/Portfolio/ivo-portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/kamen.kolev/Documents/Projects/Portfolio/ivo-portfolio/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@2.9.15_sass@1.56.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@2.9.15_sass@1.56.1/node_modules/vite/bin/vite.js" "$@"
fi
